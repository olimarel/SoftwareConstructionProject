<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<DialogPane prefHeight="170.0" prefWidth="203.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <AnchorPane prefHeight="160.0" prefWidth="203.0">
            <children>
                <TextField fx:id="fxNafn" alignment="CENTER" layoutX="21.0" layoutY="31.0" promptText="Nafn"/>
                <TextField fx:id="fxHeimilisf" alignment="CENTER" layoutX="21.0" layoutY="74.0"
                           promptText="Heimilisfang"/>
                <Label alignment="CENTER" layoutX="67.0" layoutY="-4.0" text="Nýskráning">
                    <padding>
                        <Insets top="10.0"/>
                    </padding>
                </Label>
            </children>
        </AnchorPane>
    </content>
    <ButtonType fx:id="fxILagi" buttonData="OK_DONE" text="Í lagi"/>
    <ButtonType fx:id="fxHaettaVid" buttonData="CANCEL_CLOSE" text="Hætta við"/>
</DialogPane>
